{% extends 'base.html.twig' %}

{% block title %}{{ parent() }} : User {% endblock %}

{% block body %}

    <table>
        <thead>
        <tr>
            <th>SL</th>
            <th>Username</th>
        </tr>
        </thead>
        <tbody>
        {% for user in users %}
            <tr>
                <td>{{ loop.index }}</td>
                <td>{{ user.username }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
