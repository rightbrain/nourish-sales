{% if entities is not empty %}
    {% for entity in entities %}
        <tr>
            <td>{{ entity.name }}</td>
            <td>{{ entity.status==1?"Enable":"Disable" }}</td>
            <td><button type="button" class="btn btn-primary btn-sm setting_status_change" data-value="{{ entity.status==1?0:1 }}" data-action="{{ path('setting_status_change',{"id":entity.id}) }}">Status Change</button></td>
        </tr>
    {% endfor %}
{% endif %}